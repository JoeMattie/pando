{
  "name": "@pluralsight/icons",
  "version": "0.0.0",
  "description": "Icons for Pluralsight.",
  "type": "module",
  "main": "build/index.cjs",
  "module": "build/index.js",
  "types": "build/index.d.ts",
  "exports": {
    "import": "build/wrapper.mjs",
    "require": "build/index.cjs",
    "./svg/": "./build/svg/"
  },
  "files": [
    "build/*"
  ],
  "sideEffects": [
    "build/index.cjs",
    "build/wrapper.mjs"
  ],
  "scripts": {
    "build": "yarn run build:svg",
    "build:svg": "yarn run prebuild:svg && svgo --recursive --folder src --output build/svg",
    "prebuild:svg": "rm -rf build/svg",
    "generate:react": "yarn run pregenerate:react && node ./utils/svgToReact.mjs",
    "pregenerate:react": "rm -rf build/generated/react",
    "test": "echo \"'yarn test' should be run from root directory.\" && exit 1"
  },
  "devDependencies": {
    "@svgr/core": "^6.2.1",
    "@types/react": "^17.0.40",
    "@types/svgo": "^2",
    "svgo": "^2.8.0",
    "tslib": "^2.3.1",
    "typescript": "4.4.4"
  },
  "keywords": [
    "pluralsight",
    "pluralsight design",
    "pluralsight icons",
    "design system",
    "design icons"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Design System Team",
  "bugs": {
    "url": "https://github.com/pluralsight/tva/issues"
  },
  "homepage": "https://design.pluralsight.com/",
  "repository": {
    "type": "git",
    "url": "git@github.com:pluralsight/tva.git",
    "directory": "packages/icons"
  },
  "license": "Apache 2.0",
  "packageManager": "yarn@3.1.0"
}
