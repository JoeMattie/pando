{
  "name": "@pluralsight/icons",
  "version": "0.0.0",
  "description": "Icons for Pluralsight.",
  "main": "build/index.js",
  "module": "build/wrapper.js",
  "types": "build/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/wrapper.js",
      "require": "./build/index.js"
    },
    "./react/": "./build/react/",
    "./svelte/": "./build/generated/svelte/",
    "./svelte": {
      "import": "./build/generated/svelte/wrapper.js",
      "require": "./build/generated/svelte/index.js"
    },
    "./build/generated/svelte/": "./build/generated/svelte/",
    "./svg/": "./build/svg/"
  },
  "files": [
    "build/svg/**/*",
    "build/react/**/*",
    "build/generated/svelte/**/*",
    "index.js",
    "index.d.ts",
    "wrapper.js"
  ],
  "sideEffects": [
    "build/index.js",
    "build/wrapper.js"
  ],
  "scripts": {
    "build": "yarn run build:svg && yarn run build:react && yarn run biuld:svelte && yarn run create:entryFiles",
    "build:react": "yarn generate:react && tsc --project ./tsconfig.cjs.json",
    "build:svelte": "yarn generate:svelte",
    "build:svg": "svgo --recursive --folder src --output build/svg",
    "create:entryFiles": "yarn node ./utils/reactPostbuild.mjs",
    "generate:react": "yarn node ./utils/svgToReact.mjs",
    "generate:svelte": "yarn node ./utils/svgToSvelte.mjs",
    "test": "echo \"'yarn test' should be run from root directory.\" && exit 1"
  },
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@svgr/core": "^6.2.1",
    "@types/react": "^17.0.40",
    "@types/svgo": "^2",
    "rollup": "^2.72.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^7.1.0",
    "svelte": "^3.48.0",
    "svgo": "^2.8.0",
    "typescript": "4.4.4"
  },
  "keywords": [
    "pluralsight",
    "pluralsight design",
    "pluralsight icons",
    "design system",
    "design icons"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Design System Team",
  "bugs": {
    "url": "https://github.com/pluralsight/tva/issues"
  },
  "homepage": "https://design.pluralsight.com/",
  "repository": {
    "type": "git",
    "url": "git@github.com:pluralsight/tva.git",
    "directory": "packages/icons"
  },
  "license": "Apache 2.0",
  "packageManager": "yarn@3.1.0"
}
