{
  "name": "@pluralsight/icons",
  "version": "0.0.0",
  "description": "Icons for Pluralsight.",
  "main": "build/react/index.js",
  "module": "build/react/wrapper.js",
  "types": "build/react/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/react/wrapper.js",
      "require": "./build/react/index.js"
    },
    "./react/": "./build/react/",
    "./svg/": "./build/svg/"
  },
  "files": [
    "build/*"
  ],
  "sideEffects": false,
  "scripts": {
    "postbuild": "rm -rf build/generated",
    "postbuild:react": "node ./utils/reactPostbuild.mjs",
    "build": "yarn run build:svg && yarn run build:react && yarn run postbuild",
    "build:react": "yarn run prebuild:react && tsc --project ./tsconfig.cjs.json && yarn run postbuild:react",
    "build:svg": "yarn run prebuild:svg && svgo --recursive --folder src --output build/svg",
    "prebuild:react": "rm -rf build/react && yarn run generate:react",
    "prebuild:svg": "rm -rf build/svg",
    "generate:react": "yarn run pregenerate:react && node ./utils/svgToReact.mjs",
    "pregenerate:react": "rm -rf build/generated/react",
    "test": "echo \"'yarn test' should be run from root directory.\" && exit 1"
  },
  "devDependencies": {
    "@svgr/core": "^6.2.1",
    "@types/react": "^17.0.40",
    "@types/svgo": "^2",
    "svgo": "^2.8.0",
    "typescript": "4.4.4"
  },
  "keywords": [
    "pluralsight",
    "pluralsight design",
    "pluralsight icons",
    "design system",
    "design icons"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Design System Team",
  "bugs": {
    "url": "https://github.com/pluralsight/tva/issues"
  },
  "homepage": "https://design.pluralsight.com/",
  "repository": {
    "type": "git",
    "url": "git@github.com:pluralsight/tva.git",
    "directory": "packages/icons"
  },
  "license": "Apache 2.0",
  "packageManager": "yarn@3.1.0",
  "dependencies": {
    "tslib": "^2.3.1"
  }
}
